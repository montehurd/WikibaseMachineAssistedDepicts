<div class="wbmad-image-with-suggestions-container-outer">
  <div class="wbmad-image-with-suggestions-container-inner">
    <div class="wbmad-image-with-suggestions-contents-left">
      {{{imageDescriptionLabel}}}
      <img src="{{thumburl}}" title="{{imageTagTitle}}">
    </div>
    <div class="wbmad-image-with-suggestions-container-wrap">
      <div class="wbmad-image-with-suggestions-contents-right">
        {{{suggestionsConfirmed}}}
        {{{suggestions}}}
        {{{suggestionsRejected}}}
        <div class="wbmad-buttons">{{{buttonFinish}}}{{{buttonConfirmAll}}}{{{buttonRejectAll}}}{{{buttonReset}}}</div>
      </div>
    </div>
  </div>
</div>
